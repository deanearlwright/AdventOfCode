{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
        "es6",
        "dom"
    ],
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
